datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Payment {
  amount      Float?
  createdAt   DateTime  @default(now())
  gymId       String?
  id          String    @id @default(cuid())
  paymentDate DateTime?
  updatedAt   DateTime  @updatedAt
  userId      String?
}
